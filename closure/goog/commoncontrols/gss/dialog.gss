.modal-dialog-buttons .button-text {
  padding: 11px 30px 11px 30px;
  max-width: 120px;
  line-height: 14px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: block;
}

.modal-dialog-buttons div {
  font-size: 14px;
  height: 36px;
  line-height: 36px;
  vertical-align: middle;
  text-align: center;
  border-radius: 2px;
  cursor: pointer;
  display: inline-block;
  float: right;
  color: #2785fc;
}

.modal-dialog-buttons div:hover {
  background-color: rgba(153, 153, 153, 0.2);
}


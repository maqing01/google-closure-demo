.modal-dialog {
  color: #333;
  /*position: fixed;*/
  z-index: 2001;
  background-size: contain;
  /*border: 1px solid #c4c4c4;*/
  /*border-radius: 11px;*/
  left: 50%;
  top: 50%;
  /*width: 350px;*/
  -webkit-user-select: none;
  margin: -10px;
  overflow: hidden;
  outline: none;


  -webkit-box-shadow: 0 4px 16px rgba(0,0,0,.2);
  -moz-box-shadow: 0 4px 16px rgba(0,0,0,.2);
  box-shadow: 0 4px 8px rgba(0,0,0,.32), 0 8px 40px rgba(0,0,0,.4);
  background: #fff;
  background-clip: padding-box;
  /*border: 1px solid #acacac;*/
  /*border: 1px solid rgba(0,0,0,.333);*/
  outline: 0;
  position: absolute;
  padding: 24px 32px;
}

.modal-dialog-bg {
  background-color: white;
  background-color: rgba(0, 0, 0, 0.54);
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 1990;
  pointer-events: auto;
}

.modal-dialog-title {
  /*background-color: #f8f8f9;*/
  font-weight: bold;
  font-size: 16px;
  /*font-weight: 500;*/
  line-height: 44px;
  /*text-align: center;*/
  position: relative;
  height: 44px;
  overflow: hidden;
  /*border-bottom: 1px solid #c4c4c4;*/
  cursor: default;
}

.modal-dialog-title-draggable {
  cursor: move;
}

.modal-dialog-buttons {
  display: block;
  outline: none;
  width: 100%;
  margin-top: 1px;
  padding-top: 10px;
  padding-bottom: 10px;
  background: #fff;
  text-align: center;
}

